<h1>Listing tbl_slide_layers</h1>

<table>
  <thead>
    <tr>
      <th>Slideid</th>
      <th>Layerid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tbl_slide_layers.each do |tbl_slide_layer| %>
      <tr>
        <td><%= tbl_slide_layer.SlideID %></td>
        <td><%= tbl_slide_layer.LayerID %></td>
        <td><%= link_to 'Show', tbl_slide_layer %></td>
        <td><%= link_to 'Edit', edit_tbl_slide_layer_path(tbl_slide_layer) %></td>
        <td><%= link_to 'Destroy', tbl_slide_layer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tbl slide layer', new_tbl_slide_layer_path %>
